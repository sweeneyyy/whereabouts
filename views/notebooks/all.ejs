<div class="container">
  <div class="page-header">
   <h1 class="text-center">notebooks</h1>
  </div>

  <div class="row">
    <div class="col-sm-offset-3 col-sm-6 col-sm-offset-3">
      <a href="/notebooks/new" class="btn btn-sm btn-default" id="newNotebook">new notebook</a>
    </div>
  </div>
  <br>
  <% notebookOnFrontEnd.forEach(function(item) { %>
  <div class="container">
  <div class="row">
    <div class="col-sm-6">
      <div class="panel panel-default">
        <div class="panel-body">
          <h4 id="notebook-title">
            <a href="/notebooks/<%= item.id %>">
              <%= item.title %>
            </a>
          </h4>
          <p><%= item.content %></p>
        </div>
        <div class="panel-footer">
          <span>
            <a href="/notebooks/edit/<%= item.id %>" class="btn btn-default btn-sm edit" id="editNotebook">
              <span class="glyphicon glyphicon-pencil"></span>
                EDIT
            </a>
            <a href="/notebooks/<%= item.id %>" class="btn btn-default btn-sm delete-notebook" id="delNotebook">
              <span class="glyphicon glyphicon-remove"></span>
                DELETE
            </a>
          </span>
        </div>
      </div>
    </div>
  <% }); %>
  </div>
</div>
</div>
